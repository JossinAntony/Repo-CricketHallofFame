<style type="text/css">
	.login-form {
		width: 340px;
    	margin: 50px auto;
	}
    .login-form form {
    	margin-bottom: 15px;
        background: #f7f7f7;
        box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);
        padding: 30px;
    }
    .login-form h2 {
        margin: 0 0 15px;
    }
    .form-control, .btn {
        min-height: 38px;
        border-radius: 2px;
    }
    .btn {        
        font-size: 15px;
        font-weight: bold;
    }
</style>
</head>
<body>
<div class="login-form">
    <form id="login-profile" action="/logInAPI" method="post">
        <h2 class="text-center">Log in</h2>       
        <div class="form-group">
            <input id="uname" name="uname" type="text" class="form-control" placeholder="Username is admin" required="required">
        </div>
        <div class="form-group">
            <input id="upass"  name="upass"type="password" class="form-control" placeholder="Password is 12345" required="required">
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-primary btn-block" >Log in</button>
        </div>
       
    </form>
    <a href="/signup">New member? Signup here</a>
</div>
